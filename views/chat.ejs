<div class="container">
  <div class="py-5 text-center">
    <a href="/users/logout" class="btn btn-secondary">Logout</a>
    <h2>Chat pro</h2>
    <p class="lead">Your name and start</p>
  </div>
  <div class="row">
    <div class="col-6">
      <h3>Message form</h3>
      <form action="" id="messForm">
        <label for="name">Name</label>
        <input
          type="text"
          class="form-control"
          id="name"
          name="name"
          placeholder="Your name"
        />
        <br />
        <label for="message">Message</label>
        <textarea
          name="message"
          id="message"
          class="form-control"
          placeholder="Your message"
        ></textarea>
        <br />
        <input type="submit" class="btn btn-danger" value="Send" id="submit" />
      </form>
    </div>
    <div class="col-6">
      <h3>Messages</h3>
      <div id="all_mess"></div>
    </div>
  </div>
</div>

<script src="/socket.io/socket.io.js"></script>
